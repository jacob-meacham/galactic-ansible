{
  "presets": [
    ["env", {
      "targets": {
        "node": 7.0
      },
      "useBuiltIns": true
    }]
  ],
  "plugins": [
    "transform-runtime",
    ["transform-object-rest-spread", {
      "useBuiltIns": true
    }],
    "espower",
    "add-module-exports"
  ],
  "env": {
    "development": {
      "sourceMaps": "inline"
    }
  }
}
